    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-scroll": "^1.5.5",
    "copy-webpack-plugin": "^6.0.3",
    "typescript": "^3.9.6"
  },
  "dependencies": {
